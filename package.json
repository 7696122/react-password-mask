{
  "name": "react-password-mask",
  "version": "2.0.1",
  "description": "Show/hide the contents of a password field.",
  "main": "lib/passwordMask.min.js",
  "scripts": {
    "build": "webpack --mode=build",
    "example": "webpack --config=webpack.config.example.js",
    "dev": "webpack --progress --colors --watch --mode=dev",
    "test": "mocha --compilers js:babel-core/register --colors -w ./test/*.js",
    "preversion": "npm run build && npm run example",
    "dropbox": "cp -r example/* ~/Dropbox/Public/demos/react-password-mask/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zakangelle/react-password-mask.git"
  },
  "keywords": [
    "show",
    "hide",
    "password",
    "field",
    "mask"
  ],
  "author": "Zak Angelle",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zakangelle/react-password-mask/issues"
  },
  "homepage": "https://github.com/zakangelle/react-password-mask#readme",
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "enzyme": "^2.4.1",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^3.16.1",
    "mocha": "^3.0.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "sinon": "^1.17.6",
    "webpack": "^1.13.2",
    "yargs": "^5.0.0"
  }
}
